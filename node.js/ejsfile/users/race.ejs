<%
var myMG =0;
var myAG =0;
var myyear =0;
var mymajor;
var mycollege =0;
var myorder =1;
for(var i=0; i<rows.length; i++)
{
var oneItem = rows[i];

  if(oneItem.BaseName == logname ){
  myMG = oneItem.SMajorAVG.toFixed(1);
  myAG = oneItem.SGradeAVG.toFixed(1);
  myyear = oneItem.SLevel;
  mymajor = oneItem.BaseMajor;
  }

}

for(var i=0; i<rows.length; i++)
{
  var oneItem = rows[i];
  if(oneItem.BaseMajor == mymajor){
    if(oneItem.SLevel==myyear){
      mycollege++

      if(oneItem.BaseName != logname ){
        if(myAG<oneItem.SGradeAVG){
         myorder++;
        }
        else if(myAG==oneItem.SGradeAVG){
          if(myMG < oneItem.SMajorAVG) myorder++;
        }
      }
    }
  }
}
%>
  <tr>
<td align="center"><%=logname%></td>
    <td align="center"><%=myyear%></td>
    <td align="center"><%=myMG%></td>
    <td align="center"><%=myAG%></td>
    <td align="center"><%=myorder%> / <%=mycollege%></td>
  </tr>




