<script>
$( ".star_rating a" ).click(function() {
     $(this).parent().children("a").removeClass("on");
     $(this).addClass("on").prevAll("a").addClass("on");

	document.getElementById('result').value = $(this).index() + 1;
	document.getElementById('result').innerHTML = $(this).index() + 1;
     return $(this).index() + 1;
});

</script>

